{
  "name": "difftar",
  "version": "0.0.0",
  "description": "RAWR! I'm Difftar! The giant green tarball-diffing monster that stomps through your npm packages! Fear my unified diffs!",
  "type": "module",
  "bin": {
    "difftar": "./bin/difftar.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./src/errors.js"
    }
  },
  "packageManager": "pnpm@10.26.1",
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:types": "tsc",
    "test": "node --test 'test/**/*.test.js'",
    "prepublishOnly": "npm run build:types"
  },
  "keywords": [
    "npm",
    "diff",
    "tarball",
    "edge",
    "cloudflare-workers",
    "wintercg",
    "difftar",
    "reptar",
    "rawr"
  ],
  "author": "Charlie Robbins <npm@charlie.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indexzero/difftar.git"
  },
  "dependencies": {
    "aws4fetch": "^1.0.20",
    "binary-extensions": "^3.0.0",
    "diff": "^7.0.0",
    "modern-tar": "^0.7.2"
  },
  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/node": "^24.10.1",
    "typescript": "^5.0.0"
  }
}
